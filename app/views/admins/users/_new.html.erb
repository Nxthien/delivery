<div class="content">
  <div class="panel-new">
    <div class="title"><h2>ADD RESTAURANT</h2></div>
    <div class="form-restaurant">
      <%= simple_form_for :user , url: admins_users_path, method: 'post' do |f| %>
          <br>
          <div class="row">
            <div class="col-xs-8">
              <%= f.input :name, label: "Name" %>
              <%= f.input :email, label: "Email"%>
              <%= f.input :address, label: "Address", class: "st" %>
              <%= f.input :phonenumber, label: "Phone Number"%>
            </div>
            <div class="col-xs-4">
              <%= f.input :isadmin, as: :boolean, label: "Is Admin", class: "st" %>
              .input :price_range, label: "Price Range", class: "st"%>
            </div>
            <div class="clearfix"></div>
          </div>
          <br>
          <%= f.button :submit, label: "Add Restaurant" , class: " btn btn-primary"%>
      <% end %>
    </div>
    <div class="clearfix"></div>
  </div>
</div>
