<div class="content">
  <div class="restaurant">
    <div class="detail-restaurant">
      <div class="content-info">
        <div class="title-header">
          <div class="title">
            <h2>Basic Info</h2>
          </div>
        </div>
        <dl class="block">
          <dt class="block-title">
            <span>ID</span>
          </dt>
          <dd class="block-content">
            <%= @user.id %>
          </dd>
        </dl>
        <dl class="block">
          <dt class="block-title">
            <span>Name</span>
          </dt>
          <dd class="block-content">
            <%= @user.name %>
          </dd>
        </dl>
        <dl class="block">
          <dt class="block-title">
            <span>Email</span>
          </dt>
          <dd class="block-content">
            <%= @user.email %>
          </dd>
        </dl>
        <dl class="block">
          <dt class="block-title">
            <span>Address</span>
          </dt>
          <dd class="block-content">
            <%= @user.address %>
          </dd>
        </dl>
        <dl class="block">
          <dt class="block-title">
            <span>Phone Number</span>
          </dt>
          <dd class="block-content">
            <%= @user.phonenumber %>
          </dd>
        </dl>
        <dl class="block">
          <dt class="block-title">
            <span>Admin ?</span>
          </dt>
          <dd class="block-content">
            <%= @user.isadmin %>
          </dd>
        </dl>
      </div>
    </div>
  </div>
</div>