<div class="content">
	<div class="restaurant">
		<div class="detail-restaurant">
			<div class="content-info">
			  <div class="title-header">
			  	<div class="title">
				  	<h2>Basic Info</h2>
			  	</div>
			  </div>
			  <dl class="block">
			    <dt class="block-title">
			      <span>ID</span>
			    </dt>
			    <dd class="block-content">
			      <%= @food.id %>
			    </dd>
			  </dl>
			  <dl class="block">
			    <dt class="block-title">
			      <span>Name</span>
			    </dt>
			    <dd class="block-content">
			      <%= @food.name %>
			    </dd>
			  </dl>
			  <dl class="block">
			    <dt class="block-title">
			      <span>Basic Price</span>
			    </dt>
			    <dd class="block-content">
			      <%= @food.basicprice %>
			    </dd>
			  </dl>
			  <dl class="block">
			    <dt class="block-title">
			      <span>Amount</span>
			    </dt>
			    <dd class="block-content">
						<%= @food.amount %>
				  </dd>
			  </dl>
			  <dl class="block">
			    <dt class="block-title">
			      <span>Bouns</span>
			    </dt>
			    <dd class="block-content">
						<%= @food.bouns %>
				  </dd>
			  </dl>
			  <dl class="block">
			    <dt class="block-title">
			      <span>Price</span>
			    </dt>
			    <dd class="block-content">
						<%= @food.price %>
				  </dd>
			  </dl>
			</div>
		</div>
	</div>
</div>