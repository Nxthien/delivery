<div class="content">
	<div class="restaurant">
		<div class="detail-restaurant">
			<div class="content-info">
			  <div class="title-header">
			  	<h2>Basic Info</h2>
			  </div>
			  <dl class="block">
			    <dt class="block-title">
			      <span>ID</span>
			    </dt>
			    <dd class="block-content">
			      <%= @restaurant.id %>
			    </dd>
			  </dl>
			  <dl class="block">
			    <dt class="block-title">
			      <span>Name</span>
			    </dt>
			    <dd class="block-content">
			      <%= @restaurant.name %>
			    </dd>
			  </dl>
			  <dl class="block">
			    <dt class="block-title">
			      <span>Type Restaurant</span>
			    </dt>
			    <dd class="block-content">
			      <%= @restaurant.type_restaurant %>
			    </dd>
			  </dl>
			  <dl class="block">
			    <dt class="block-title">
			      <span>Address</span>
			    </dt>
			    <dd class="block-content">
			      <%= @restaurant.address %>
			    </dd>
			  </dl>
			  <dl class="block">
			    <dt class="block-title">
			      <span>Time Open</span>
			    </dt>
			    <dd class="block-content">
			      <%= @restaurant.time_open %>
			    </dd>
			  </dl>
			  <dl class="block">
			    <dt class="block-title">
			      <span>Time Close</span>
			    </dt>
			    <dd class="block-content">
			      <%= @restaurant.time_close %>
			    </dd>
			  </dl>
			  <dl class="block">
			    <dt class="block-title">
			      <span>Price Range</span>
			    </dt>
			    <dd class="block-content">
			      <%= @restaurant.price_range %>
			    </dd>
			  </dl>
			</div>
		</div>
	</div>
</div>